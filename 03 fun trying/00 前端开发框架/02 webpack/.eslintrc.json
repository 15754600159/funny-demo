{
	"parser": "babel-eslint",
	"extends": "airbnb",
	"env": {
		"browser": true,
		"es6": true
	},
	"rules": {
		"linebreak-style": [
			"off",
			"windows"
		],
		"indent": [
			2,
			4
		],
		"react/jsx-indent": [
			2,
			4
		],
		"one-var": "off",
		"no-class-assign": "off",
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
		"react/prop-types": "off",
		"jsx-a11y/anchor-is-valid": "off"
	}
}