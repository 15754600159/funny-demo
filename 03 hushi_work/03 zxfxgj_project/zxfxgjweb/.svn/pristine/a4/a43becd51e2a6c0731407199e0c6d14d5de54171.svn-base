<div class="container bgmask">
	<div class="window" style="width: 600px; height: 80%;" id="window-qtdwfx-viewWindow">
		<header>
			<h1>挖掘结果信息查看</h1>
			<a class="close btn-window-close">×</a>
		</header>
		<div class="content">
			<form class="form-view qtdwfx viewWindows" id="view-qtdwfx-info">
				<section class="h-box col2">
				</section>
			</form>
		</div>
		<footer>
			<a class="btn btn-outline btn-window-close">关闭</a>
		</footer>
	</div>
</div>

