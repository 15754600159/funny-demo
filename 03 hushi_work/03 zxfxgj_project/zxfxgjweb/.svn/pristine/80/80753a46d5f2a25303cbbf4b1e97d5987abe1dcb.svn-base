.gzqt_body {
	background: #fff;
	.gzqt_head {
		width: 100%;
		height: 36px;
		line-height: 36px;
		background: #4f8cf6;
		h2 {
			line-height: 36px;
			font-size: 14px;
			margin-left: 20px
		}
	}
	.gzqt_cont {
		margin-top: 14px;
		.gzqt_tj {
			width: 96%;
			margin: 0 auto;
			background: #fcfcfc;
			border: 1px solid #dfdfdf;
			border-top: none;
			.tj_head {
				height: 32px;
				line-height: 32px;
				background: #deeaff;

				p {
					font-size: 14px;
					margin-left: 20px;
					line-height: 32px;
					color: #000;
				}
			}
			.tj_body {
				width: 99%;
				margin: 20px auto;
				.form-inline {
					input {
						width: 160px;
					}
					.form-group {
						margin-left: 20px;
					}
				}
			}
		}
		.gzqt_all {
			position: relative;
			width: 96%;
			margin: 0 auto;
			.search-btn-div {
				margin: 0 auto;
				margin-top: 20px;
				padding-bottom: 50px;
				.left-btn-side {
					position: absolute;
				}
				.right-btn-side {
					position: absolute;
					right: 0;
				}
				.qtgzbd {
					padding-top: 48px;
				}
				.fenye {
					position: absolute;
					right: 0px;
					bottom: -5px;
				}
			}
		}
	}
}
