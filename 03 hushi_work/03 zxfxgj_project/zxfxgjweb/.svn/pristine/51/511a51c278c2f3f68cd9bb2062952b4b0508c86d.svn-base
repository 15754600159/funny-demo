<div class="container bgmask">
	<div class="window" style="width: 50%; height: 50%;" id="window-zxrygl-addWindow">
		<header style="background:#4f8cf6">
			<h1 style="color:#fff">新增人员采集</h1>
			<a class="close btn-window-close">&times;</a>
		</header>
		<div class="content" style="padding:20px;">
			<form id="form-user-add" class="form">
				<section class="h-box col2">
					<div>
						<div class="control-item">
							<label class="required">姓名:</label>
							<div>
								<input type="text" class="xm" name="name">
							</div>
						</div>
					</div>
					<div>
						<div class="control-item">
							<label class="required">别名绰号:</label>
							<div>
								<input type="text" class="bmch" name="bmch">
							</div>
						</div>
					</div>
					<div class="control-item">
						<label>性别:</label>
						<div>
							<select class="xb" name="sex">
								<option value ="">--</option>
								<option value ="man">男</option>
								<option value ="woman">女</option>
							</select>
						</div>
					</div>
					<div class="control-item">
						<label>出生日期:</label>
						<input size="16" type="text" readonly class="form_datetime" id="birday" style="width:240;">
					</div>
					<div class="control-item">
						<label>身份证号:</label>
						<div>
							<input type="text" class="sfzh" name="sfzh">
						</div>
					</div>
					<div class="control-item">
						<label>民族:</label>
						<div>
							<select class="select" name="nationCode" id="user-add-nationCode">
									<option value="">--</option>
							</select>
						</div>
					</div>
					<div class="control-item">
						<label>人员类别:</label>
						<div>
							<select class="select" name="politicsStatus" id="user-add-politicsStatus">
									<option value="">--</option>	
									<option value="涉疆">涉疆</option>
									<option value="涉毒">涉毒</option>
									<option value="涉恐">涉恐</option>
								</select>
						</div>
					</div>
					<div class="control-item">
						<label>列控原因:</label>
						<div>
							<input type="text" class="lkyy" name="mobile">
						</div>
					</div>
					<div class="control-item">
						<label>联系方式:</label>
						<div>
							<input type="text" class="lxfs" name="officeTel">
						</div>
					</div>
					<div class="control-item">
						<label>交通方式:</label>
						<div>
							<input type="text" class="jtfs" name="policeNo">
						</div>
					</div>
					<div class="control-item">
						<label>户籍地区划:</label>
						<div>
							<input type="text" class="hjdqh" name="hjdqh">
						</div>
					</div>
					<div class="control-item">
						<label>居住地区划:</label>
						<div>
							<input type="text" class="jzdqh" name="jzdqh">
						</div>
					</div>
					<div class="control-item">
						<label>户籍地祥址:</label>
						<div>
							<input type="text" class="hjdxz" name="hjdxz">
						</div>
					</div>
					<div class="control-item">
						<label>居住地祥址:</label>
						<div>
							<input type="text" class="jzdxz" name="jzdxz">
						</div>
					</div>
					<div class="control-item">
						<label>户籍地派出所:</label>
						<div>
							<div>
								<input type="text" class="hjdpcs" name="hjdpcs">
							</div>
						</div>
					</div>
					<div class="control-item">
						<label>居住地派出所:</label>
						<div>
							<input type="text" class="jzdpcs" name="jzdpcs">
						</div>
					</div>
					<div class="control-item">
						<label>证件类型:</label>
						<div>
							<input type="text" class="zjlx" name="zjlx">
						</div>
					</div>
					<div class="control-item">
						<label>证件号码:</label>
						<div>
							<input type="text" class="zjhm" name="zjhm">
						</div>
					</div>
					<div class="control-item">
						<label>是否共享:</label>
						<div>
							<select class="gongxiang">
										<option value="0">-共享-</option>	
										<option value="1">-打马可见-</option>
										<option value="2">不可见</option>
									</select>
						</div>
					</div>
					<div class="control-item">
						<label>是否获取全国轨迹:</label>
						<div>
							<select class="guiji">
									<option value="0">-是-</option>	
									<option value="1">-否-</option>
								</select>
						</div>
					</div>
					<div class="control-item">
						<label>采集人员:</label>
						<div>
							<input type="text" class="caiji" name="caiji">
						</div>
					</div>
					<div class="control-item">
						<label>采集人联系方式:</label>
						<div>
							<input type="text" class="caijiph" name="caijiph">
						</div>
					</div>
					<div class="control-item">
						<label>采集单位（代码）:</label>
						<div>
							<input type="text" class="caijicode" name="caijicode">
						</div>
					</div>
					<div class="control-item">
						<label>采集时间:</label>
						<div>
							<input size="16" type="text" readonly class="form_datetime" id="cjsj" style="width:240;">
						</div>
					</div>
			</form>
		</div>
		<footer style="background:#4f8cf6">
			<a class="btn" id="btn-ok-person-excel">确定</a>
			<a class="btn btn-outline btn-window-close">取消</a>
		</footer>
	</div>
</div>

<script>
	app.zxrygl.add.init();

</script>
