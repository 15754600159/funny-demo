<div class="container bgmask">
    <div class="window" style="width: 50%; height: 50%;" id="window-zxrygl-addWindows">
        <header style="background:#4f8cf6">
            <h1 style="color:#fff">新增人员采集</h1>
            <a class="close btn-window-close">&times;</a>
        </header>
        <div class="content" style="padding:20px;    margin: 50px 0 8px 0">
            <form id="form-user-add" class="form">
                <section class="h-box col2">
                    <div>
                        <div class="control-item">
                            <label class="required">姓名:(必填)</label>
                            <div>
                                <input type="text" class="xm" name="name" readonly>
                            </div>
                        </div>
                    </div>
                    <div>
                        <div class="control-item">
                            <label>别名绰号:</label>
                            <div>
                                <input type="text" class="bmch" name="bmch" readonly>
                            </div>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>性别:</label>
                        <div>
                            <select class="xb" name="sex" disabled="disabled">
								<option value ="">--</option>
							</select>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>出生日期:</label>
                        <input size="16" type="text" readonly class="form_datetime" id="birday" style="width:240;">
                    </div>
                    <div class="control-item">
                        <label class="required">身份证号:(必填)</label>
                        <div>
                            <input type="text" class="sfzh" name="sfzh" readonly>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>民族:</label>
                        <div>
                            <select class="select" name="nationCode" id="user-add-nationCode" disabled="disabled">
									<option value="">--</option>
							</select>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>人员类别:</label>
                        <div>
                            <select class="select" name="politicsStatus" id="user-add-politicsStatus" disabled="disabled">
									<option value="">--</option>	
								</select>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>列控原因:</label>
                        <div>
                            <input type="text" class="lkyy" name="mobile" readonly>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>联系方式:</label>
                        <div>
                            <input type="text" class="lxfs" name="officeTel" readonly>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>交通方式:</label>
                        <div>
                            <input type="text" class="jtfs" name="policeNo" readonly>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>户籍地区划:</label>
                        <div>
                            <input readonly id="citySel" code="" type="text" class="form-control2" placeholder='户籍地区划' />
                            <input id="ucodeName" type="hidden">
                        </div>
                    </div>
                    <div class="control-item">
                        <label>居住地区划:</label>
                        <div>
                            <input readonly id="jzdSel" code="" type="text" class="form-control2" placeholder='居住地区划' />
                            <input id="jzdName" type="hidden">
                        </div>
                    </div>
                    <div class="control-item">
                        <label>户籍地祥址:</label>
                        <div>
                            <input type="text" class="hjdxz" name="hjdxz" readonly>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>居住地祥址:</label>
                        <div>
                            <input type="text" class="jzdxz" name="jzdxz" readonly>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>户籍地派出所:</label>
                        <div>
                            <div>
                                <input type="text" class="hjdpcs" name="hjdpcs" readonly>
                            </div>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>居住地派出所:</label>
                        <div>
                            <input type="text" class="jzdpcs" name="jzdpcs" readonly>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>证件类型:</label>
                        <div>
                            <select name="politicsStatus" class="zjlx" disabled="disabled">
								<option value="">--</option>	
							</select>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>证件号码:</label>
                        <div>
                            <input type="text" class="zjhm" name="zjhm" readonly>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>是否共享:</label>
                        <div>
                            <select class="gongxiang" disabled="disabled">
										<option value="0">-共享-</option>	
										<option value="1">-打马可见-</option>
										<option value="2">不可见</option>
									</select>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>是否获取全国轨迹:</label>
                        <div>
                            <select class="guiji" disabled="disabled">
									<option value="0">-是-</option>	
									<option value="1">-否-</option>
								</select>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>采集人员:</label>
                        <div>
                            <input type="text" class="caiji" name="caiji" readonly>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>采集人联系方式:</label>
                        <div>
                            <input type="text" class="caijiph" name="caijiph" readonly>
                        </div>
                    </div>
                    <div class="control-item">
                        <label>采集单位（代码）:</label>
                        <div>
                            <input id="cjdwcode" code="" readonly type="text" class="form-control2" placeholder='采集单位' />
                            <input id="ucodecjdw" type="hidden">
                        </div>
                    </div>
                    <div class="control-item">
                        <label class="required">采集时间:(必填)</label>
                        <div>
                            <input size="16" type="text" readonly class="form_datetime" id="cjsj" style="width:240;" readonly>
                        </div>
                    </div>
            </form>
        </div>
    </div>
</div>
<script>
    app.zxrygl.add.viewinit();

</script>
