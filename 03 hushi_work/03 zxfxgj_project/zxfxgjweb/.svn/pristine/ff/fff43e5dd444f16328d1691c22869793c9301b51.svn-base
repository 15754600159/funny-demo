<!-- 群体挖掘列表 -->
<script type="text/html" id="tmpl-list-qtwj-search">
		<!--% for (var i in o) { %-->
		<tr>
			<td style="text-align: center">
				<label class="check check-nolabel">
					<input type="checkbox" value="{%=o[i].id%}" name="id"><span></span>
				</label>
			</td>
			<td>{%=o[i].rwId%}</td>
			<td>{%=o[i].qtName%}</td>
			<td>{%=o[i].qtgm%}</td>
			<td>{%=o[i].qtcy%}</td>
			<td>{%=o[i].qtlx%}</td>
			<td>{%=o[i].source%}</td>
			<td>{%=o[i].wjsj%}</td>
			<td>{%=o[i].description%}</td>
			<td>
				<button class="btn btn-sm btn-info" type="button" style="margin-left:10px;" onclick="app.qtwj.search.openPerson('{%=o[i].id%}');">人员</button>
				<button class="btn btn-sm btn-primary look" type="button" onclick="app.qtwj.search.openView('{%=o[i].id%}');">查看</button>
			</td>
		</tr>
		<!--% } %-->
</script>

