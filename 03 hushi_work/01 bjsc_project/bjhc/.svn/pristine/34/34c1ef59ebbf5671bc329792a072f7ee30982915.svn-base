<!DOCTYPE HTML>
<html lang="zh-cn">

<head>
      <title>任务管理</title>
      <meta charset="utf-8">
      <link rel="stylesheet" type="text/css" href="../css/plugins/bootstrap.min.css" />
      <link rel="stylesheet" type="text/css" href="../css/backgroundReview.css" />
</head>

<body>
      <div class="all">
            <div class="top">
                  <a href="#"><img src="../img/nav-logo.png"></a>
                  <div class="title">人员背景核查</div>
                  <div class="role">
                        <a href="#">admin</a>
                  </div>
            </div>
            <div class="container-fluid content">
                  <form class="task-management">
                        <input type="text" class="form-control" placeholder='请输入要查找的人物名' id="chaxun_input">
                        <button type="button" class="btn btn-default" id="chaxun">查询</button>
                        <button type="button" class="btn btn-default">背景审查</button>
                  </form>
                  <table class="table table-bordered table-striped table-hover" id="task_table">
                        <thead>
                              <tr>
                                    <th>任务名</th>
                                    <th>上传人</th>
                                    <th>上传时间</th>
                                    <th>上传条数</th>
                                    <th>审查进度</th>
                                    <th></th>
                                    <th></th>
                              </tr>
                        </thead>
                        <tbody id="task_tbody"></tbody>
                  </table>
                  <!-- 分页 -->
                  <div style="text-align: right">
                        <nav style="width: 100%;">
                              <ul id="pagination" class="pagination"></ul>
                        </nav>
                  </div>
            </div>
      </div>
      <script type="text/javascript" src="../js/plugins/jquery-1.9.1.min.js"></script>
      <script type="text/javascript" src="../js/plugins/bootstrap.min.js"></script>
      <script src="../js/plugins/jquery.tablesorter.min.js"></script>
      <script type="text/javascript" src="../js/plugins/fetch.js"></script>
      <script type="text/javascript" src="../js/plugins/moment.js"></script>
      <script type="text/javascript" src="../js/plugins/bootstrap-pagination.min.js"></script>
      <script type="text/javascript" src="../js/web/task.js"></script>
</body>

</html>
