@font-face {
  font-family: 'icomoon';
  src: url('../fonts/icomoon.eot?7ktl3t');
  src: url('../fonts/icomoon.eot?7ktl3t#iefix') format('embedded-opentype'), url('../fonts/icomoon.ttf?7ktl3t') format('truetype'), url('../fonts/icomoon.woff?7ktl3t') format('woff'), url('../../fonts/icomoon.svg?7ktl3t#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}
.check-label > input {
    position: absolute;
    left: -300px;
}
.check-label {
    position: relative;
    display: inline-block;
    padding: 6px 6px 6px 22px;
    margin: 0;
    overflow: hidden;
    min-width: 28px;
    min-height: 28px;
}
.check-label > input[type="checkbox"]:checked + span:before {
    content:"\e90a";
    font-family: 'icomoon';
    color: #1baff7;
    font-size: 24px;
    line-height: 18px;
    position: absolute;
    z-index: 1;
    left: -1px;
}
.check-label > input[type="checkbox"]:checked + span:after {
    border-color: #1baff7;
    text-align: center;
}
.check-label > input[type="checkbox"] + span:after {
    top: 4px;
    width: 18px;
    height: 18px;
    margin: 1px;
}
.check-label > span:after {
    content: "";
    background-color: transparent;
    outline: none;
    border-radius: 0;
    display: block;
    position: absolute;
    top: 5px;
    left: 0;
    width: 16px;
    height: 16px;
    border: 1px solid #177ab3;
}

#ck_btn{
	position:absolute;
	right:30px;
	bottom: 0px;
	left:inherit;
	margin-bottom: 12px
}
