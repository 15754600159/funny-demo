package com.minginglamp.model;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;
import lombok.ToString;

@Data
@ToString
@Entity
@Table(name="local_concern_person")
public class LocalConcernPerson {

	
	private static final long serialVersionUID = -3641426206379490180L;
	@Id
	@GeneratedValue
	private Integer id;
	
	 @Column(name = "idcard_no")
	private String idcardNo;
	
	 @Column(name = "name")
	private String name;
	 
	 @Column(name = "sex")
	private String sex;
	 
	 @Column(name = "national")
	private String national;
	 
	 @Column(name = "address_details")
	private String addressDetails;
	 
	 @Column(name = "domicile")
	private String domicile;
	 
	 @Column(name = "personneltype")
	private String personneltype;
	 
	
}
