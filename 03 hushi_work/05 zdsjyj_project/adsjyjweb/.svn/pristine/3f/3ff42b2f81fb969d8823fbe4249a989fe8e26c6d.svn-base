<div class="page">
    <div class="gzqt_body">
        <div class="gzqt_head">
            <h2>群体挖掘</h2>
        </div>
        <div class="gzqt_cont">
            <div class="gzqt_tj">
                <div class="tj_head">
                    <p>过滤条件</p>
                </div>
                <div class="tj_body">
                    <form class="form-search-bar" id="form-qtwj-search">
						<div class="control-item">
							<label class="required">挖掘时间:</label>
							<div>
								<input size="16" type="text" readonly class="form_datetime" id="" name="beginWjsj" style="width: 230px;"> --
								<input size="16" type="text" readonly class="form_datetime" id="" name="endWjsj" style="width: 230px;">
							</div>
						</div>
						<div class="control-item">
							<label class="required">任务记录编号:</label>
							<div>
								<input id="" name="rwId" type="text" class="input" placeholder="请输入任务记录编号">
							</div>
						</div>
						<div class="control-item">
							<label class="required">群体名称:</label>
							<div>
								<input id="" name="qtName" type="text" class="input" placeholder="请输入群体名称">
							</div>
						</div>
						<!-- <div class="control-item">
							<label>是否本地:</label>
							<div>
								<label class="check">
									<input type="checkbox" name="" value="" checked="checked">
									<span>是</span>
								</label>
								<label class="check">
									<input type="checkbox" name="" value="">
									<span>否</span>
								</label>
							</div>
						</div> -->
					</form>
                </div>
            </div>
            <div class="gzqt_all">
                <div class="search-btn-div">
                    <div class="left-btn-side">
                     	<!-- <button type="button" class="btn btn-searchbar" id="form-qtwj-import">导入</button> -->
                        <button type="button" class="btn btn-searchbar" id="form-qtwj-export">导出</button>
                        <button type="button" class="btn btn-searchbar" >群体多维分析</button>
                        <button type="button" class="btn btn-searchbar" id="form-qtwj-refresh">刷新</button>
                    </div>
                    <div class="right-btn-side">
                        <button type="button" class="btn btn-submit" id="form-qtwj-submit">查询</button>
                        <button type="button" class="btn btn-submit" id="form-qtwj-reset">重置</button>
                    </div>
                    <div class="qtgzbd">
                        <table class="table table-striped table-bordered">
                            <thead>
                                <tr>
                                	<th><label class="check check-nolabel">
											<input type="checkbox" id="list-qtwj-search-checkAll"><span></span>
										</label>
									</th>
                                    <th style="width: 10%;">任务记录编号</th>
                                    <th style="width: 10%;">群体名称</th>
                                    <th style="width: 10%;">群体规模</th>
                                    <th style="width: 10%;">群体成因</th>
                                    <th style="width: 10%;">群体类型</th>
                                    <th style="width: 14%;">数据来源</th>
                                    <th style="width: 9%;">挖掘时间</th>
                                    <th style="width: 10%;">描述</th>
                                    <th style="width: 13%;">操作</th>
                                </tr>
                            </thead>
                            <tbody id="list-qtwj-search">
                               
                            </tbody>
                        </table>
                        <ul id="qtwj-search-pagebtn" class="pagination">
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- 群体挖掘列表 -->
<script type="text/html" id="tmpl-list-qtwj-search">
		<!--% for (var i in o) { %-->
		<tr>
			<td style="text-align: center">
				<label class="check check-nolabel">
					<input type="checkbox" value="{%=o[i].id%}" name="id"><span></span>
				</label>
			</td>
			<td>{%=o[i].rwId%}</td>
			<td>{%=o[i].qtName%}</td>
			<td>{%=o[i].qtgm%}</td>
			<td>{%=o[i].qtcy%}</td>
			<td>{%=o[i].qtlx%}</td>
			<td>{%=o[i].source%}</td>
			<td>{%=o[i].wjsj%}</td>
			<td>{%=o[i].description%}</td>
			<td>
				<button class="btn btn-sm btn-info" type="button" style="margin-left:10px;" onclick="app.qtwj.search.openPerson('{%=o[i].id%}');">人员</button>
				<button class="btn btn-sm btn-primary look" type="button" onclick="app.qtwj.search.openView('{%=o[i].id%}');">查看</button>
			</td>
		</tr>
		<!--% } %-->
</script>

<script>
    app.qtwj.search.init();
</script>



