<div class="page">
    <div class="rygl">
        <div class="gzqt_head">
            <h2>未关注人员列表</h2>
        </div>
        <form class="form-search-bar" id="form-gzqtwgzrygl-search">
            <div class="control-item">
                <label>群体编号</label>
                <div>
                    <input id="qtrywgzgl-qtbh" name="zdqtbh" type="text" class="input" placeholder="请输入身份证号">
                </div>
            </div>
            <div class="control-item">
                <label>身份证号</label>
                <div>
                    <input id="qtrywgzgl-sfzh" name="sfzh" type="text" class="input" placeholder="请输入身份证号">
                </div>
            </div>
            <div class="control-item">
                <label class="required">姓名</label>
                <div>
                    <input id="qtrywgzgl-name" name="xm" type="text" class="input" placeholder="请输入姓名">
                </div>
            </div>

            <div class="control-item">
                <label>标签</label>
                <div>
                    <input id="qtrywgzgl-label" name="tag" type="text" class="input" placeholder="请输入姓名">
                </div>
            </div>
            <div class="search-btn-div">
                <div class="left-btn-side">
                    <button type="button" class="btn btn-searchbar" onclick="app.gzqt.toqtrywgzAddPage(app.gzqt.gzqtrywgzSearch.qtbh)">添加</button>
                    <button type="button" class="btn btn-searchbar" onclick="app.gzqt.gzqtrywgzdel.del()">删除</button>
                    <!-- <button type="button" class="btn btn-searchbar">下载模版</button>
                    <button type="button" class="btn btn-searchbar">导入</button>
                    <button type="button" class="btn btn-searchbar">导出</button> -->
                    <button type="button" class="btn btn-searchbar" onclick="app.gzqt.gzqtrywgzSearch.init()">刷新</button>
                    <button type="button" class="btn btn-searchbar" onclick="app.gzqt.gzqtrywgzSearch.viewback()">返回</button>
                    <!-- //viewback -->

                </div>
                <div class="right-btn-side">
                    <button type="button" class="btn btn-submit" onclick="app.gzqt.gzqtrywgzSearch.initForm()">查询</button>
                    <button type="button" class="btn btn-submit" onclick="app.gzqt.gzqtrywgzSearch.reset()">重置</button>
                </div>
            </div>
        </form>

        <div id="list-qtrygl">
            <div class="qtgzbd">
                <table class="table table-striped table-bordered">
                    <thead>
                        <tr>
                            <th></th>
                            <th>群体编号</th>
                            <th>身份证号</th>
                            <th>姓名</th>
                            <th>活动轨迹</th>
                            <th>成员类型</th>
                            <th>成员标签</th>
                            <th>操作</th>
                        </tr>
                    </thead>
                    <tbody class="table_all" id="qtrywgzgl_table">
                    </tbody>
                </table>
                <ul id="gzqtrywgzpagination" class="pagination">
                </ul>
            </div>
        </div>
    </div>


    <script>
        app.gzqt.gzqtrywgzSearch.init();
    </script>

</div>
